{
  "name": "gulp-preload-image",
  "version": "0.0.1",
  "description": "A gulp extension to preload images via CSS3.",
  "main": "./index.js",
  "proxy": null,
  "https-proxy": null,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "scripts": {
      "test": "make test",
      "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
      "test-travis": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Xalior/gulp-preload-image.git"
  },
  "keywords": [
    "image",
    "preload",
    "css",
    "javascript",
    "html",
    "gulpplugin",
    "eventstream"
  ],
  "author": {
    "name": "Darran D. Rimron-Soutter",
    "email": "darran@xalior.com",
    "url": "http://github.com/Xalior"
  },
  "contributors": [
    {
      "name": "Waterside Development",
      "url": "http://github.com/watersidedevelopment"
    }
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/Xalior/gulp-preload-image/issues"
  },
  "keywords": [
    "gulp",
    "preload",
    "image",
    "css"
  ],
  "dependencies": {
    "coveralls": "^2.11.2",
    "event-stream": "*",
    "gulp-util": "*",
    "istanbul": "^0.3.5",
    "mocha-lcov-reporter": "0.0.1"
  },
  "devDependencies": {
    "mocha": "*",
    "should": "*",
    "tmp": "0.0.24"
  }
}
